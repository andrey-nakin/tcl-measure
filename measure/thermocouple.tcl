proc measure::thermocouple::calc { tc fixedT v } {
	eval "set data \$measure::thermocouple::$tc"

	set minT [lindex $data 0]
	set maxT [lindex $data 1]
	set step [lindex $data 2]
	set data [lindex $data 3]
	set maxIdx [llength $data]

	if { $fixedT < $minT || $fixedT > $maxT } {
		return 0.0
	}

	set idx [expr int(floor(($fixedT - $minT) / $step))]
	set t0 [expr $minT + $step * $idx]
	if { $idx > 0 && $idx < $maxIdx - 2 } {
		# cubic spline interpolation
		set coeffs [::math::interpolate::prepare-cubic-splines [list [expr $t0 - $step] $t0 [expr $t0 + $step] [expr $t0 + 2.0 * $step]] [lrange $data $idx-1 $idx+2]]
		set base [::math::interpolate::interp-cubic-splines $coeffs $fixedT]
	} elseif { $idx < $maxIdx - 1} {
		# linear interpolation
		set v0 [lindex $data $idx]
		set v1 [lindex $data $idx+1]
		set base [expr $v0 + ($v1 - $v0) * ($fixedT - $t0) / $step]
	} else {
		# linear interpolation
		set v0 [lindex $data $idx-1]
		set v1 [lindex $data $idx]
		set base [expr $v0 + ($v1 - $v0) * ($fixedT - $t0) / $step]
	}
	set v [expr $base + 1000.0 * $v]

	set idx [::measure::bsearch::lowerBound $data $v]
	if { $idx < 0 } {
		return 0.0
	}

	set t0 [expr $idx * $step + $minT]
	if { $idx > 0 && $idx < $maxIdx - 2 } {
		# cubic spline interpolation
		set coeffs [::math::interpolate::prepare-cubic-splines [lrange $data $idx-1 $idx+2] [list [expr $t0 - $step] $t0 [expr $t0 + $step] [expr $t0 + 2.0 * $step]] ]
		set t [::math::interpolate::interp-cubic-splines $coeffs $v]
	} elseif { $idx < $maxIdx - 1 } {
		# linear interpolation
		set v0 [lindex $data $idx]
		set v1 [lindex $data $idx+1]
		set t [expr $t0 + $step * ($v - $v0) / ($v1 - $v0)]
	} else {
		# linear interpolation
		set v0 [lindex $data $idx-1]
		set v1 [lindex $data $idx]
		set t [expr $t0 + $step * ($v - $v0) / ($v1 - $v0)]
	}
	return $t
}


proc measure::thermocouple::makeTcData { fileName } {
	global measure::thermocouple::ZERO_CELCIUM

	set fd [open $fileName r]
	set src [read $fd]
	close $fd

	set lines [split $src "\n"]
	set prev ""
	set data [list]
	set first ""

	foreach line $lines {
		set values [regexp -inline -all -- {\S+} [string trim $line]]
		if { [llength $values] < 13 } {
			continue
		}
		set t [lindex $values 0]
		if { $t == "" || ![string is double $t] } {
			continue
		}

		if { $t >= 0 && ($prev == "" || $prev >= 0) } {
			if { $first == "" } {
				set first $t
			}

			for { set i 1 } { $i <= 10 } { incr i } {
				lappend data [lindex $values $i]
			}
		} else {
			if { $first == "" } {
				set first [expr $t - 10]
			}

			for { set i 11 } { $i >= 2 } { incr i -1 } {
				lappend data [lindex $values $i]
			}
		}

		set prev $t
	}

	puts "\[list [expr $first + $ZERO_CELCIUM] [expr $first + [llength $data] - 1 + $ZERO_CELCIUM] 1.0 \[list \\"
	set l [llength $data]
	for { set i 0 } { $i < $l } { incr i } {
		puts -nonewline "[lindex $data $i] "
		if { $i % 10 == 9 } {
			puts " \\"
		}
	}
	puts "\]\]"
}

